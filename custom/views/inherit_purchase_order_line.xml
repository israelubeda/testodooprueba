<odoo>
    <data>
        <record id="view_purchase_order_line_tree" model="ir.ui.view">
            <field name="name">view.purchase.order.line.tree</field>
            <field name="model">purchase.order</field>
            <field name="inherit_id" ref="purchase.purchase_order_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='product_id']" position="after">
                        <field name="filed_l5"/>

                </xpath>
                <xpath expr="//field[@name='partner_ref']" position="after">
                        <field name="filed_l6"/>

                </xpath>
                
                
            </field>
        </record>
    </data>
    
        <data>
        <record id="view_account_move_line_tree" model="ir.ui.view">
            <field name="name">view.account.move.line.form</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='product_id']" position="after">
                        <field name="filed_l4"/>

                </xpath>
            </field>
        </record>
    </data>
    
</odoo>